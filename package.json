{
  "description": "A picker for date and time, that prefers to use the native input. It includes a calendar and a timer.",
  "keywords": [
    "webcomponents",
    "polymer",
    "picker",
    "datetime",
    "date",
    "calendar",
    "time",
    "timer",
    "range",
    "input",
    "polyfill"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fooloomanzoo/datetime-picker.git"
  },
  "homepage": "https://github.com/fooloomanzoo/datetime-picker/",
  "name": "@fooloomanzoo/datetime-picker",
  "version": "3.0.8",
  "authors": [
    "Johannes Brautzsch <fooloomanzoo>"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && git add . && git commit -m \"publish new version\" && npm version patch && git push",
    "build": "git pull && npm update -f && npm run analyze && webpack --env production",
    "start": "npm run analyze && webpack-dev-server --env development --open",
    "analyze": "polymer analyze > analysis.json",
    "test": "polymer test",
    "test:p": "polymer test -p --config-file=wct.p.conf.js"
  },
  "license": "MIT",
  "dependencies": {
    "@fooloomanzoo/datetime-input": "^3.0.4",
    "@fooloomanzoo/input-picker-pattern": "^3.0.10",
    "@fooloomanzoo/number-input": "^3.0.10",
    "@fooloomanzoo/property-mixins": "^3.0.10",
    "@polymer/polymer": "^3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-template-html-minifier": "^2.0.1",
    "babel-preset-minify": "^0.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "fast-async": "^6.3.8",
    "@webcomponents/webcomponentsjs": "^2",
    "@polymer/iron-component-page": "^4",
    "@polymer/iron-flex-layout": "^3",
    "@polymer/iron-demo-helpers": "^3",
    "@polymer/iron-form": "^3",
    "@polymer/iron-test-helpers": "^3",
    "wct-browser-legacy": "^1",
    "terser-webpack-plugin": "^1.1.0",
    "uglify-template-string-loader": "^1.1.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "author": "Johannes Brautzsch <fooloomanzoo>"
}
