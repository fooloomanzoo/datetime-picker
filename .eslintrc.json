{
  "extends": "eslint:recommended",
  "root": true,
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-var": "error",
    "no-undef": [ "error", {
      "typeof": true
    } ],
    "no-console": "off",
    "valid-jsdoc": [ "error", {
      "requireReturn": false,
      "prefer": {
        "arg": "param",
        "argument": "param",
        "returns": "return"
      },
      "requireParamDescription": false,
      "preferType": {
        "Boolean": "boolean",
        "Number": "number",
        "String": "string",
        "object": "Object"
      }
    } ]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [ "html" ],
  "globals": {
    "customElements": true,
    "HTMLImports": true,
    "Polymer": true,
    "ShadyDOM": true,
    "ShadyCSS": true,
    "JSCompiler_renameProperty": true,
    "WorkerGlobalScope": true
  }
}
